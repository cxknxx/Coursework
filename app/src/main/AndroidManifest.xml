<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="gcu.mpd.cknox202.coursework">

    <uses-permission android:name="android.permission.INTERNET" />

    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity android:name="gcu.mpd.cknox202.coursework.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="gcu.mpd.cknox202.coursework.activities.HomeActivity"/>
        <activity android:name="gcu.mpd.cknox202.coursework.activities.ItemActivity"/>
        <activity android:name="gcu.mpd.cknox202.coursework.activities.ResultActivity"/>
    </application>

</manifest>